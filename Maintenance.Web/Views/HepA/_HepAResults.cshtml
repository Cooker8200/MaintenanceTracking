@model IEnumerable<Maintenance.Models.ViewModels.HepA_ViewModels>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstShot)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecondShot)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstShot)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondShot)
        </td>
    </tr>
}

</table>
